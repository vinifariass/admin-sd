generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum TipoMorador {
  Proprietario
  Inquilino
}

model Parking {
  id          String      @id @default(uuid())
  apartamento String
  placa       String
  carro       String
  cor         String
  createdAt   DateTime    @default(now())
  cpf         String
  tipoMorador TipoMorador
  nome        String
}

model Morador {
  id          String    @id @default(uuid())
  nome        String
  cpf         String
  apartamento String
  email       String?
  telefone    String?
  dataLocacao DateTime
  dataSaida   DateTime?
  createdAt   DateTime  @default(now())
}

